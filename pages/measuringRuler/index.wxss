.page-section {
  margin-bottom: 20rpx;
}

.weui-input {
  height: 2.58823529em;
  min-height: 2.58823529em;
  line-height: 2.58823529em;
}

.weui-cells__title {
  margin-top: 0.77em;
  margin-bottom: 0.3em;
  padding-left: 15px;
  padding-right: 15px;
  color: #000;
  font-size: 14px;
}

.weui-cells {
  position: relative;
  margin-top: 1.17647059em;
  background-color: #fff;
  line-height: 1.41176471;
  font-size: 12px;
}

.weui-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #d9d9d9;
  color: #d9d9d9;
}

.weui-cells:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1rpx solid #d9d9d9;
  color: #d9d9d9;
}

.weui-cells__title {
  margin-top: 0.77em;
  margin-bottom: 0.3em;
  padding-left: 15px;
  padding-right: 15px;
  color: #999;
  font-size: 14px;
}

.weui-cells_after-title {
  margin-top: 0;
}

.weui-cells__tips {
  margin-top: 0.3em;
  color: #999;
  padding-left: 15px;
  padding-right: 15px;
  font-size: 14px;
}

.weui-cell_input {
  padding-top: 0;
  padding-bottom: 0;
}

.weui-cell {
  padding: 2px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

button {
  overflow: unset;
}

.moView {
  background-color: #fff;
  font-size: 12px;
  position: relative;
  text-align: center;
}

.width_pos {
  position: absolute;
  left: 0;
  top: -20px;
  text-align: center;
}

.width_pos > label {
  background-color: #eaeaea;
  padding: 0 5px;
}

.width_pos > view {
  height: 1px;
  border-top: 1px dotted #000;
  position: absolute;
  top: 8px;
  z-index: -1;
}

.long_pos {
  position: absolute;
  left: -14px;
  top: 0;
  display: flex;
  align-items: center;
}

.long_pos > label {
  display: inline-block;
  word-break:break-word;
  width: 10px;
  background-color: #eaeaea;
  line-height: 11px;
  margin-right: 2px;
}

.long_pos > view {
  position: absolute;
  z-index: -1;
  top: 0;
  border-left: 1px dotted #000;
  right: 6px;
}

.picker {
  font-size: 14px;
  line-height: 30px;
  padding-left: 15px;
}

.btnType {
  float: left;
  width: 750rpx;
  text-align: center;
  display: flex;
  line-height: 50px;
}
.btnType > view{
  flex:1;
}
.bunpri{
  display: inline-block;
  padding:0 5px;
  height: 30px;
  line-height: 30px;
  font-size: 12px;
}
.body-view{
  padding:15rpx;
}