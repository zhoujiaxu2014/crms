/* 弹出框样式修改 */

.asdfskjdklf .van-popup {
background-color: var(--overlay-background-color,rgba(0,0,0,.0))!important;
height: 100%;
}

@-webkit-keyframes rotate {
	0% {
		transform: matrix3d(0.9, 0, 0, 0, 0, 0.9, 0, 0, 0, 0, 1, 0, 60, 0, 0, 1);
		z-index: 101;
	}

	25% {
		transform: matrix3d(1.23333, 0, 0, 0, 0, 1.23333, 0, 0, 0, 0, 1, 0, -50, 10, 0, 1);
		z-index: 101.1;
	}

	50% {
		transform: matrix3d(0.9, 0, 0, 0, 0, 0.9, 0, 0, 0, 0, 1, 0, -150, 12, 0, 1);
		z-index: 100;
	}

	75% {
		transform: matrix3d(0.56667, 0, 0, 0, 0, 0.56667, 0, 0, 0, 0, 1, 0, -50, 10, 0, 1);
		z-index: 99;
	}


	100% {
		transform: matrix3d(0.90003, 0, 0, 0, 0, 0.90003, 0, 0, 0, 0, 1, 0, 60, 0.00827, 0, 1);
		z-index: 101.00008;
	}
}

