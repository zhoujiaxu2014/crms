  /**
   * 页面的初始数据
   */
  data: {
    navIndex:0,
  },
  /*
  *切换导航栏
   */
  clickNav:function(e){
    debugger
    if (e.currentTarget.dataset.index==1){
      this.setData({
        navIndex:1
      })
    } else if (e.currentTarget.dataset.index == 2){
      this.setData({
        navIndex: 2
      })
    } else if (e.currentTarget.dataset.index == 3) {
      this.setData({
        navIndex: 3
      })
    } else{
      this.setData({
        navIndex: 0
      })
    }
  },

