<view class="activity">
  <view class="activity_list" wx:for='{{activeList}}' wx:key='{{index}}'>
    <view class="activity_list_left">
      <van-image width="240rpx" height="200rpx" src="../../../../image/best.jpg" custom-class="my-activities-image" />
    </view>
    <view class="activity_list_right">
      <view style="color: #343434;font-size: 16px;">{{item.activityName}}</view>
      <view style="color: #8C8C8C;font-size: 13px;">{{item.item.startTime}}-{{item.item.endTime}}</view>
      <view class="activity_des">{{item.activityDesc}}</view>
    </view>
  </view>
