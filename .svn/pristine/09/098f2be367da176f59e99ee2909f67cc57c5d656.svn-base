page {
  background-color: #f4f4f4;
}
.base-info-wrapper {
  height: 200rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  /* background-image: url('');
  background-repeat:no-repeat; background-size:100% 100%;-moz-background-size:100% 100%; */
}

.base-info-up {
  height: 200rpx;
  display: flex;
  align-items: center;
}
.index_home .dialog-index--van-dialog {
  z-index: 10000;
}
/* 头像 */

.headSculpture {
  margin-left: 40rpx;
  margin-right: 30rpx;
}

/* 登录/注册 */

.loginOrRegister {
  font-size: 32rpx;
  font-weight: bold;
  padding: 0;
  margin: 0;
  flex: 1;
  background: transparent;
  text-align: left;
  letter-spacing: 2rpx;
}

.loginOrRegister::after,
.messageBtn::after {
  border: none !important;
}

/* 提示消息 */
.messageBtn {
  display: flex;
  line-height: 40rpx !important;
  color: #fff !important;
  font-size: 24rpx !important;
  border-radius: 40rpx 0 0 40rpx !important;
  background: linear-gradient(to right, #f2b042 0%, #ee8031 100%);
  border: none !important;
  padding: 10rpx 20rpx;
}

/* 我的装修需求单 */

.my-demand-list-wrapper {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
  text-align: center;
  padding: 20px 0;
}
.my-demand-list-wrapper .iconfont {
  font-size: 36px;
  margin-bottom: 10px;
}
.my-demand-list-wrapper view {
  font-size: 12px;
}
.title_room {
  color: black !important;
}
.room_pass {
  color: #f7b528;
  margin-right: 10px;
}
.look_more {
  line-height: 60px;
  text-align: center;
  color: #a2a2a2;
  border-top: 1px solid #e4e4e4;
  background-color: #fff;
  font-size: 12px;
}
.room_list {
  margin-top: 20rpx;
  text-align: center;
  background-color: #fff;
  margin-bottom: 100rpx;
  padding: 0 0 100rpx 0;
}
.room_list > h1 {
  color: #131336;
  font-weight: bold;
  font-size: 28rpx;
  letter-spacing: 1rpx;
  line-height: 120rpx;
}
.img_list {
  width: 100%;
  text-align: left;
}
.img_box {
  display: inline-block;
  width: 33%;
  margin-top: 10px;
  text-align: center;
}
.img_box > .list_name_word {
  font-size: 12px;
  margin-top: 5px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
