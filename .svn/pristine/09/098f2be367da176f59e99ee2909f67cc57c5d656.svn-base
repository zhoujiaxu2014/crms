.nothing {
  line-height: 80px;
  width: 100%;
  height: 100rpx;
}
.right_icon {
  position: relative;
}
.icon_status {
  position: absolute;
  top: 15px;
  right: 40px;
  color: #e4e4e4;
  font-size: 46px;
}
.myRoom_list > ul > li {
  font-size: 11px;
}



.rowDemand .van-row {
  margin: 0!important;
  text-align: left;
  font-size: 16px;
}
.offer_item {
  margin-bottom: 50px;
}
/* tab栏 */
.tabBar_box {
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  text-align: center;
  font-size: 12px;
  padding:  20rpx 0;
  border-top: 1px solid #e4e4e4;
  background-color: #FFFFFF;
  color: #B7B7B7;
  z-index: 9999;
}
.tabBar_box > view {
  width: 25%;
}
.tabBar_box > view >image {
  height: 50rpx;
  width: 50rpx;
  z-index: 999999999;
}