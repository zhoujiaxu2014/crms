.demand-list-merit {
  display: flex;
  align-items: center;
  width: 100%;
  margin: 20rpx 0;
}

.demand-list-merit > view {
  flex: 1;
  display: flex;
  flex-direction: column;
  text-align: center;
}

.demand-list-icon {
  font-size: 24px;
  padding: 10rpx 0;
}

.demand-list-merit > view > span:first-of-type {
  font-size: 24rpx;
  color: #1d1d1d;
  line-height: 40rpx;
}

.demand-list-merit > view > span:last-of-type {
  font-size: 20rpx;
  color: #a6a6a6;
  line-height: 40rpx;
}

.addDemandBtn {
  padding: 0 40rpx !important;
  margin-bottom: 40rpx;
  margin-top: 10rpx;
  letter-spacing: 4rpx;
}

/* 我的参与的活动 */

.my-activities-wrapper {
  margin-top: 20rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 20rpx;
}

.my-activities-wrapper > h1 {
  color: #131336;
  font-weight: bold;
  font-size: 28rpx;
  letter-spacing: 1rpx;
  line-height: 80rpx;
}

.my-activities-item {
  width: 100%;
  margin-top: 10px;
}

.my-activities-title {
  font-size: 12px;
  color: rgb(19, 19, 19);
}

.my-activities-title > span {
  display: inline-block;
  width: 4rpx;
  height: 30rpx;
  background-color: #84c6e9;
  margin-right: 10rpx;
  margin-bottom: -4rpx;
}

.my-activities-content {
  width: 100%;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
  margin-top: 10px;
}

.my-activities-image {
  margin: 0 10rpx;
}

.my-activities-content > ul {
  display: flex;
  flex-direction: column;
}

.my-activities-content > ul > li {
  font-size: 24rpx;
  line-height: 40rpx;
  color: rgb(153, 153, 153);
}

.shouquan {
  width: 100%;
  height: 200rpx;
  line-height: 200rpx;
  text-align: center;
}

.shouquan .van-icon {
  font-size: 20px;
  color: #ee8031;
  margin-right: 10px;
}

.itemDemond {
  margin-top: 10px;
  width: 100%;
}

.dmontItem {
  border: 1px solid #e4e4e4;
  padding: 10px 20px;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}

.dmontItem > ul {
  display: flex;
  flex-direction: column;
}

.dmontItem > ul > li {
  font-size: 24rpx;
  line-height: 40rpx;
  color: rgb(153, 153, 153);
}

.nomoreData {
  text-align: center;
  font-size: 25rpx;
  margin-top: 40rpx;
  color: #a2a2a2;
}

.tabBar {
  display: flex;
  justify-content: space-around;
  font-size: 25rpx;
  position: fixed;
  bottom: 0;
  right: 0;
  left: 0;
  border-top: 1px solid #f7f7f7;
  padding: 10px;
  background: #fff;
}

.tabBar > view {
  display: flex;
  flex-direction: column;
  text-align: center;
}

button {
  border: 0 !important;
  line-height: 1.2 !important;
}

.headImg {
  height: 150rpx;
  width: 150rpx;
  border-radius: 50%;
  margin-left: 60rpx;
  margin-right: 30rpx;
}

.my-demand-list-wrapper .van-button__text {
  line-height: 22px;
}

.active {
  color: #ef8532;
}

